<%- include partials/header.ejs %>

<h1>Travel Log</h1>
<div>
  <p>Stats</p>
</div>
<div>
  <ul>
    <% travelLog.forEach((country)=>{ %>
    <li><%= country.name %></li>
    <form action="/travellog/<%= country.id%>/delete" method="POST">
      <input type="submit" value="Delete">    
    </form>
    <% }) %>
  </ul>
  <a href="/travellog/add">Add</a>
</div>
